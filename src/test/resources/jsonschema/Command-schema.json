{
	"$id": "jsonschema:jsonschema/Command",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"kind",
		"version",
		"payload"
	],
	"properties": {
		"kind": {
			"type": "string"
		},
		"version": {
			"type": "string"
		},

		"dryrun": {
			"type": "boolean"
		},
		"verbosity": {
			"enum": [
				"HIGH",
				"MEDIUM",
				"LOW"
			]
		},
		"payload": {
			"type" : "object",
			"required" : [

			],
			"properties" : {
				"subject"   : { "$ref": "jsonschema:jsonschema/Subject"   },
				"predicate" : { "$ref": "jsonschema:jsonschema/Predicate" }
			},
			"additionalProperties": false
		}
	},
	"additionalProperties": false
}
