{
	"$id": "jsonschema:com.mukadobo.json.schema.poc.command.Command",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"kind",
		"version",
		"payload"
	],
	"properties": {
		"kind": {
			"type": "string"
		},
		"version": {
			"type": "string"
		},

		"dryrun": {
			"type": "boolean"
		},
		"verbosity": {
			"enum": [
				"OFF",
				"FATAL",
				"ERROR",
				"WARN",
				"INFO",
				"DEBUG",
				"TRACE",
				"ALL",
			]
		},
		"payload": {
			"type" : "object",
			"required" : [

			],
			"properties" : {
				"subject"   : { "$ref": "jsonschema:com.mukadobo.json.schema.poc.command.Subject"   },
				"predicate" : { "$ref": "jsonschema:com.mukadobo.json.schema.poc.command.Predicate" }
			},
			"additionalProperties": false
		}
	},
}
